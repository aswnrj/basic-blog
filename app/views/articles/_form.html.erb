<%= render 'errors' %>

<%= form_with(model: @article, local: true) do |f| %>
  <p>
    <%= f.label :title, "Title" %> <br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :description, "Description" %> <br>
    <%= f.text_area :description %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>